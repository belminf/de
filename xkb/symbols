        actions[Group1]= [ NoAction(), NoAction(), RedirectKey(key=<LatF>,mods=Control,clearmods=Super) ]
    };
    key <AB01> {
        symbols[Group1]= [          z,          Z,                                             NoSymbol ],
        actions[Group1]= [ NoAction(), NoAction(), RedirectKey(key=<LatZ>,mods=Control,clearmods=Super) ]
    };
    key <AB02> {
        repeat= Yes,
        symbols[Group1]= [          x,          X,                                             NoSymbol ],
        actions[Group1]= [ NoAction(), NoAction(), RedirectKey(key=<LatX>,mods=Control,clearmods=Super) ]
