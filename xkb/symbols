    name[Group1]="Winremap";
    key.type[Group1]="ALPHA_WIN";
    key.repeat=yes;

    key <AC01> {
        symbols[Group1]= [          a,          A,                                             NoSymbol ],
        actions[Group1]= [ NoAction(), NoAction(), RedirectKey(key=<LatA>,mods=Control,clearmods=Super) ]
