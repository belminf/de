default partial xkb_types "win_remap" {
    type "ALPHABETIC_WIN" {
        modifiers= Shift+Lock+Mod4;
        map[Shift]= Level2;
        map[Lock]= Level2;
        map[Mod4]= Level3;
        level_name[Level1]= "Base";
        level_name[Level2]= "Caps";
        level_name[Level3]= "Win";
    };
};
